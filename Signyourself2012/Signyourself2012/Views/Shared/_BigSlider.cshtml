@model  IEnumerable<Signyourself2012.Models.File>
<!-- slider -->
<section class="project-slider">
        <div class="flexslider">

            <ul class="slides">
                <li>
                    <img src="~/Images/slides/01.jpg" alt="alt text" />
                </li>
                @{

                    foreach (Signyourself2012.Models.File f in Model.Where(f => f.FileTypeId == 2))
                    {
                        <li>
                            <img src="@f.Url" alt="@f.Name" />
                        </li>
                    }
                }
            </ul>
        </div>
    </section>
<!-- ENDS slider -->